{
  "name": "notes-app-be",
  "version": "1.0.0",
  "description": "Notes App Express Backend",
  "main": "dist/index.js",
  "scripts": {
    "test:e2e": "vitest -c ./vitest.config.e2e.mts --run",
    "test:e2e:coverage": "vitest -c ./vitest.config.e2e.mts --coverage --run",
    "test:integration": "vitest -c ./vitest.config.integration.mts --run",
    "test:integration:coverage": "vitest -c ./vitest.config.integration.mts --coverage --run",
    "test:component": "vitest -c ./vitest.config.component.mts --run",
    "test:component:coverage": "vitest -c ./vitest.config.component.mts --coverage --run",
    "test:contract": "vitest --testTimeout 30000 **/contract/*.spec.ts --run",
    "test:unit": "vitest -c ./vitest.config.unit.mts --run",
    "test:unit:coverage": "vitest -c ./vitest.config.unit.mts --run --coverage",
    "start:local": "nodemon",
    "start": "node dist/index.js",
    "build": "tsc",
    "prisma": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start:dev": "tsx watch src/index.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@faker-js/faker": "^8.4.1",
    "@pact-foundation/pact": "^13.2.0",
    "@pact-foundation/pact-node": "^10.18.0",
    "@testcontainers/postgresql": "^10.14.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "20.10.6",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-istanbul": "1.2.1",
    "@vitest/coverage-v8": "1.2.1",
    "axios": "^1.7.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.1.0",
    "nodemon": "3.0.2",
    "nyc": "15.1.0",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "prisma": "5.7.1",
    "supertest": "6.3.4",
    "ts-node": "10.9.2",
    "tsx": "^4.19.2",
    "typescript": "5.3.3",
    "typescript-eslint": "^7.8.0",
    "vitest": "1.2.1",
    "vitest-mock-extended": "1.3.1"
  },
  "dependencies": {
    "@prisma/client": "5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.3.2",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2"
  }
}
